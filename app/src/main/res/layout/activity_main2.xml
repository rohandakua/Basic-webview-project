<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="@color/bg"
    android:background="@color/black"
    tools:context=".MainActivity2">

    <androidx.cardview.widget.CardView
        android:layout_width="380dp"
        android:layout_height="650dp"
        android:backgroundTint="@color/mt"
        android:elevation="100dp"
        app:cardCornerRadius="15dp"
        app:cardUseCompatPadding="true"
        android:id="@+id/cvP"
        app:layout_constraintBottom_toTopOf="@id/btn2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.197">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.cardview.widget.CardView
                android:layout_width="170dp"
                android:layout_height="140dp"
                app:cardCornerRadius="10dp"
                app:cardUseCompatPadding="true"
                android:backgroundTint="@color/mt2"
                android:id="@+id/cv2"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintLeft_toRightOf="@id/cv1"
                app:layout_constraintBottom_toTopOf="@id/cv4"
                tools:layout_editor_absoluteX="194dp"
                tools:layout_editor_absoluteY="45dp">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="@string/backend"
                    android:gravity="center"
                    android:textSize="25dp"
                    android:fontFamily="sans-serif-smallcaps"
                    android:textColor="@color/design_default_color_secondary_variant"


                    ></TextView>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="170dp"
                android:layout_height="140dp"
                tools:layout_editor_absoluteX="2dp"
                android:id="@+id/cv1"
                app:cardCornerRadius="10dp"
                app:cardUseCompatPadding="true"
                android:backgroundTint="@color/mt2"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toTopOf="@id/cv3"
                app:layout_constraintRight_toLeftOf="@id/cv2"
                app:layout_constraintLeft_toLeftOf="parent"


                tools:layout_editor_absoluteY="45dp">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="@string/Android"
                    android:gravity="center"
                    android:textSize="25dp"
                    android:fontFamily="sans-serif-smallcaps"
                    android:textColor="@color/design_default_color_secondary_variant"


                    ></TextView>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="170dp"
                android:layout_height="140dp"
                app:cardCornerRadius="10dp"
                app:cardUseCompatPadding="true"
                android:backgroundTint="@color/mt2"
                app:layout_constraintTop_toBottomOf="@id/cv2"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintLeft_toRightOf="@id/cv3"
                app:layout_constraintBottom_toTopOf="@id/cv6"
                android:id="@+id/cv4"
                tools:layout_editor_absoluteX="194dp"
                tools:layout_editor_absoluteY="246dp">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="@string/blockchain"
                    android:gravity="center"
                    android:textSize="25dp"
                    android:fontFamily="sans-serif-smallcaps"
                    android:textColor="@color/design_default_color_secondary_variant"


                    ></TextView>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="170dp"
                android:layout_height="140dp"
                app:cardCornerRadius="10dp"
                app:cardUseCompatPadding="true"
                app:layout_constraintTop_toBottomOf="@id/cv3"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toLeftOf="@id/cv6"
                android:backgroundTint="@color/mt2"
                android:id="@+id/cv5"
                tools:layout_editor_absoluteX="2dp"
                tools:layout_editor_absoluteY="454dp">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="@string/flutter"
                    android:gravity="center"
                    android:textSize="25dp"
                    android:fontFamily="sans-serif-smallcaps"
                    android:textColor="@color/design_default_color_secondary_variant"


                    ></TextView>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="170dp"
                android:layout_height="140dp"
                app:cardCornerRadius="10dp"
                app:layout_constraintTop_toBottomOf="@id/cv4"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintLeft_toRightOf="@id/cv5"
                app:layout_constraintBottom_toBottomOf="parent"
                app:cardUseCompatPadding="true"
                android:backgroundTint="@color/mt2"
                android:id="@+id/cv6"
                tools:layout_editor_absoluteX="194dp"
                tools:layout_editor_absoluteY="453dp">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="@string/frontend"
                    android:gravity="center"
                    android:textSize="25dp"
                    android:fontFamily="sans-serif-smallcaps"
                    android:textColor="@color/design_default_color_secondary_variant"


                    ></TextView>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="170dp"
                android:layout_height="140dp"
                app:cardCornerRadius="10dp"
                app:cardUseCompatPadding="true"
                android:backgroundTint="@color/mt2"
                android:id="@+id/cv3"
                app:layout_constraintTop_toBottomOf="@id/cv1"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toLeftOf="@id/cv4"
                app:layout_constraintBottom_toTopOf="@id/cv5"
                tools:layout_editor_absoluteX="2dp"
                tools:layout_editor_absoluteY="247dp">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="@string/devops"
                    android:gravity="center"
                    android:textSize="25dp"
                    android:fontFamily="sans-serif-smallcaps"
                    android:textColor="@color/design_default_color_secondary_variant"


                    ></TextView>

            </androidx.cardview.widget.CardView>


        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.cardview.widget.CardView>

    <Button
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="-16dp"
        android:id="@+id/btn2"
        android:text="Contact Us"
        android:textSize="25dp"

        android:textColor="@color/rohan"
        android:backgroundTint="@color/mt3"
        app:layout_constraintTop_toBottomOf="@id/cvP"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="676dp">


    </Button>

</androidx.constraintlayout.widget.ConstraintLayout>